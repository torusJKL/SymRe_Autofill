{
    "manifest_version": 2,
    "name": "SymRe Autofill",
    "version": "1.0",
    "description": "Automatically fills the fields for the SymRe URL shortener.",
    "homepage_url": "http://sym.re",

    "permissions": [
        "activeTab"
      ],
  
    "icons": {
        "16": "icons/SymRe_icon_16px.png",
        "48": "icons/SymRe_icon_48px.png",
        "128": "icons/SymRe_icon_128px.png"
    },
  
    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": {
            "19": "icons/SymRe_icon_19px.png",
            "38": "icons/SymRe_icon_38px.png"
          },
        "default_title": "Open SymRe & fill in values",
        "browser_style": true
    }
  }
