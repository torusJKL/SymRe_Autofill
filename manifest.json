{
    "manifest_version": 2,
    "name": "SymRe Autofill",
    "version": "1.0",
    "description": "Automatically fills the fields for the SymRe URL shortener.",
    "homepage_url": "http://sym.re",

    "permissions": [
        "activeTab"
      ],
  
    "icons": {
      "48": "icons/SymRe_favicon_64px.png"
    },
  
    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": "icons/SymRe_favicon_64px.png",
        "default_title": "Open SymRe & fill in values",
        "browser_style": true
    }
  }
